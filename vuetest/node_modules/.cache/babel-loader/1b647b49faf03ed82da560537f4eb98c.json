W,mBAAX,CARJ,EAUI,UAVJ,G,kBAeK,YAAA,CAIM,SAJN,EAIM,IAJN,EAfL,WAAA,CAekC,KAAA,CAAA,KAflC,EAAA,UAekB,SAflB,EAe4B,CAf5B,EAe6B;yBAAxB,YAAA,CAIM,KAJN,EAIM;AAJ+B,MAAA,GAAG,EAAE;AAIpC,KAJN,EAA2C,CACxC,YAAA,CAA8C,KAA9C,EAA8C;AAAxC,MAAA,GAAG,EAAE,KAAA,CAAA,KAAA,CAAM,CAAN,EAAS,KAA0B;AAAnB,MAAA,KAAK,EAAC;AAAa,KAA9C,E,IAAA,E;;AAAA,M,OAAA,CADwC,EAExC,YAAA,CAAsE,IAAtE,EAAsE;AAAjE,MAAA,OAAK,EAAA,iBAAA,MAAA,EAAA;AAAE,QAAA,KAAA,CAAA,WAAA,GAAW,IAAX;AAAoB,QAAA,KAAA,CAAA,MAAA,GAAS,CAAT;AAAU;AAA4B,KAAtE,E,iBAAgD,KAAA,CAAA,KAAA,CAAM,CAAN,EAAS,K,CAAzD,EAA8D;AAAA;AAA9D,MAA8D,CAAA,SAAA,CAA9D,CAFwC,EAGxC,YAAA,CAA6B,GAA7B,EAA6B,IAA7B,EAA6B,gBAAA,CAAvB,KAAA,CAAA,KAAA,CAAM,CAAN,EAAS,KAAc,CAAA,GAAN,IAAvB,EAAyB;AAAA;AAAzB,KAHwC,CAA3C,C;GAfL,CAeK,E;;AAAA,GAfL,EAAA,E;;AAAA,G","sourcesContent":["<template>\n \n    <!-- 데이터를 전송해주고, 등록하고 사용한다. props : 자식에게 데이터 보내는 방법 -->\n    <Modal :rooms=\"rooms\" :roomid=\"roomid\" :modalstatus=\"modalstatus\" @close=\"handlePointStorageClose\"/>\n\n    <div class=\"menu\">\n        <a v-for=\"(menu,i) in menulist\" :key=\"i\">{{ menu }}</a>\n    </div>\n\n    <!-- 컴포넌트 : 코드가 아름다워 -->\n    <Discount/>\n\n    <div style=\"text-align: center; margin: auto;\">\n        <img alt=\"Vue logo\" style=\"width: 50%;\" src=\"./assets/mojjilogo.jpg\" />\n    </div>\n\n     \n     <div v-for=\"(roomslist,i) in rooms\" :key=\"i\" >\n        <img :src=\"rooms[i].image\" class=\"room-img\" />\n        <h4 @click=\"modalstatus = true; roomid = i \">{{ rooms[i].title }}</h4>\n        <p>{{ rooms[i].price }} 원</p>    \n     </div>\n\n\n</template>\n\n<script>\n    \n    import { data } from './assets/oneroom.js'\n    import Discount from './components/Discount.vue'\n    import Modal from './components/Modal.vue'\n    \n    export default {\n        name: 'App',\n        data() {\n            return {\n                //데이터 보관함 데이터 바인딩 실시간 자동 렌더링을 하기위해서임\n                //동적인 UI만드는법 : \n                // 1. UI의 현재 상태를 데이터로 저장해둠\n                // 2. 현재 데이터에 따라 UI가 어떻게 보일지 작성\n                roomid : 0,\n                rooms : data,\n                modalstatus : false,\n                price1: 60,\n                price2: 80,\n                price3: 100,\n                cnt: [0, 0, 0],\n                products: ['역삼동원룸', '천호동원룸', '마포구원룸'],\n                menulist: ['Home', 'Shop', 'About'],\n            };\n        },\n        methods: {\n            //함수는 여기서 다 만들어준다.\n            increase() {\n                this.cnt += 1;\n            },\n            handlePointStorageClose(close){\n                this.modalstatus = close;\n            }\n        },\n        components: {\n            // 컴포넌트 등록하기\n            Discount : Discount,\n            Modal : Modal,\n        },\n    };\n</script>\n\n<style>\n    #app {\n        font-family: Avenir, Helvetica, Arial, sans-serif;\n        -webkit-font-smoothing: antialiased;\n        -moz-osx-font-smoothing: grayscale;\n        text-align: center;\n        color: #2c3e50;\n        margin-top: 60px;\n    }\n\n    .menu {\n        background: darkslateblue;\n        padding: 15px;\n        border-radius: 5px;\n    }\n\n    .menu a {\n        color: white;\n        padding: 10px;\n    }\n\n    .room-img {\n        width: 100%;\n        margin-top: 40px;\n    }\n    \n    body{\n        margin : 0\n    }\n    div {\n        box-sizing: border-box;\n    }\n    .black-bg{\n        width: 100%; height: 100%;\n        background: rgba(0,0,0,0,5);\n        position: fixed; padding: 20px\n    }\n    .white-bg{\n        width: 100%; background: white;\n        border-radius: 8px;\n        padding: 20px;\n    }\n    \n\n</style>"],"sourceRoot":""}]}